{
  "name": "synytskiy/laravel-kendo-ui",
  "description": "",
  "authors": [
    {
      "name": "Oleksa Synytskiy",
      "email": "alexsynytskiy@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable" : true,
  "require": {
    "php": ">=5.3.0"
  },
  "autoload": {
    "classmap": [
      "vendor/synytskiy/laravel-kendo-ui/wrappers/php/lib/Kendo/Autoload.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize",
      "ln -sf vendor/synytskiy/laravel-kendo-ui/wrappers/php/content/ public/vendor/synytskiy/"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize",
      "ln -sf vendor/synytskiy/laravel-kendo-ui/wrappers/php/content/ public/vendor/synytskiy/"
    ]
  }
}